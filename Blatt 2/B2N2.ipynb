   "source": [
    "import matplotlib.pyplot as plt\n",
    "r = 20\n",
    "h = 0.1\n",
    "K = 100\n",
    "N = 5\n",
    "t = 0\n",
    "def N_ext(N_old):\n",
    "    return r*N_old*(1-(N_old/K))\n",
    "while t<=5:\n",
    "    plt.plot(t,N,'bo')\n",
    "    N=N+(1/2)*h*(N_ext(N)+N_ext(N+h*N_ext(N)))\n",
    "    t=t+1"
   ]
  }
